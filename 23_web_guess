<script>
    var currentCategory = document.querySelector(".page").getAttribute("data-querystring").replace("cgid=", "")
    var theCountry = document.querySelector("html").getAttribute("data-country")
    var cgid = "https://www.guess.eu/es-es/guess/mujeres" //default cgid for header, links to guess category
    var cgidW = "https://www.guess.eu/es-es/guess/mujeres" //default cgid for button women inside of the pre-main
    var cgidM = "https://www.guess.eu/es-es/guess/hombres" //default cgid for button man inside of the pre-main
    var newsletterLink = "https://www.guess.eu/es-es/newsletter"
    var theLanguage = document.querySelector("html").getAttribute("data-lang")
    var perc, headerMsg, bannerMsg
    var promoName = "Sale" //MODIFY put promo name here that will be injected for tracking in banner
    var mssOnlineMessage

     if (theCountry == "MT" || theCountry == "TR") {
         perc = "30%";
         if (theLanguage == "tr") {
             perc = "%30"
         }
     } else {
         perc = "50%";
     }

    /*Online Message*/

    if (
        theCountry == "NO" ||
        theCountry == "RS" ||
        theCountry == "BG" ||
        theCountry == "BH" ||
        theCountry == "LT" ||
        theCountry == "HR" ||
        theCountry == "LV" ||
        theCountry == "UA" ||
        theCountry == "SA" ||
        theCountry == "QA" ||
        theCountry == "SI" ||
        theCountry == "BY" ||
        theCountry == "MD" ||
        theCountry == "ME" ||
        theCountry == "SK" ||
        theCountry == "IE" ||
        theCountry == "MK" ||
        theCountry == "EE" ||
        theCountry == "EG" ||
        theCountry == "AE" ||
        theCountry == "MT" ||
        theCountry == "AL" ||
        theCountry == "KW" ||
        theCountry == "LB" ||
        theCountry == "CZ" ||
        theCountry == "AZ" ||
        theCountry == "RO"
    ) {
