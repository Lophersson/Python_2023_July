<script>
  
    //schedule(".campaignpe22", "02/07/2022")
    var theCountry = document.querySelector("html").getAttribute("data-country") || "GB"
    
    if(theCountry == "FR") {
       schedule(".focus--FR", "02/27/2023")
       }
    
    function schedule(selector, mmddyyyy) {
        var GivenDate = new Date( mmddyyyy + " 00:00:00 GMT+0100");
        var CurrentDate = new Date();
        if (GivenDate > CurrentDate) {
            //scheduled
            document.querySelector(selector).style.display = "none"
        } else {
            //ready
if (document.querySelector(selector)) {
 document.querySelector(selector).style.display = "block"
}
           
        }
    }
    
</script>
</div>
